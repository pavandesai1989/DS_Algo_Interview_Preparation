//Time o(n) and space o(n)

class Solution {
public:
    int majorityElement(vector<int>& nums) 
    {
        int ans = -1;
        unordered_map<int,int> mp;
        for(int i=0;i<nums.size();i++)
        {
            mp[nums[i]]++;
        }
        
        for(auto ele: mp)
        {
            if(ele.second > nums.size()/2)  //important line
                return ele.first;
        }
        
        return ans;
        
    }
};
