#include <iostream>
using namespace std;
int main() 
{
    int row;
    int col;

    int a[100][100];

    cout<<"enter number of rows and columns"<<endl;
    cin>>row>>col;

    for(int i=0;i<row;i++)
    {
        for(int j=0;j<col;j++)
        {
             cin>>a[i][j];
        }
    }

    cout<<"printing the 2d array"<<endl;
    for(int i=0;i<row;i++)
    {
        for(int j=0;j<col;j++)
        {
            cout<<a[i][j]<<" ";
        }
        cout<<endl;
    }

            
    cout<<"Hello World!";
}
