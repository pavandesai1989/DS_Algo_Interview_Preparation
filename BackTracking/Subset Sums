// { Driver Code Starts
#include<bits/stdc++.h> 
using namespace std; 

 // } Driver Code Ends
class Solution
{
private:
 void helper(vector<int> arr,int N,int idx,int sum,vector<int> &ans)
 {
     if(idx == N)
     {
         ans.push_back(sum);
         return;
     }
     
     helper(arr,N,idx+1,sum+arr[idx],ans);
     
     helper(arr,N,idx+1,sum,ans);
 }
    
public:
    vector<int> subsetSums(vector<int> arr, int N)
    {
        vector<int> res;
        int sum = 0;
        
        helper(arr,N,0,0,res);
        
        sort(res.begin(),res.end());
        
        return res;
    }
};

// { Driver Code Starts.
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        int N;
        cin>>N;
        vector<int> arr(N);
        for(int i = 0 ; i < N ; i++){
            cin >> arr[i];
        }
        Solution ob;
        vector<int> ans = ob.subsetSums(arr,N);
        sort(ans.begin(),ans.end());
        for(auto sum : ans){
            cout<< sum<<" ";
        }
        cout<<endl;
    }
    return 0;
}  // } Driver Code Ends
